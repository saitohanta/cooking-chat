会員ID
<%= @member.id %>
氏名
<%= @member.name %>
メールアドレス
<%= @member.email %>
<%= form_with model: @member, url: admin_member_path(@member) do |f| %>
  会員ステータス
  <%= f.radio_button :is_deleted, :false, checked: true %>
  <%= f.label :status_false, "有効" %>
  <%= f.radio_button :is_deleted, :true %>
  <%= f.label :status_true, "退会" %>
  <%= f.submit "変更を保存" %>
<% end %>