<%= render "public/posts/error_message", post: @post %>
<%= form_with model: @post, url: posts_path do |f| %>
<%= f.label :画像 %>
<%= f.file_field :post_image, accept: "image/*" %>
<%= f.label :タイトル %>
<%= f.text_field :title, placeholder: "タイトル" %>
<%= f.label :本文 %>
<%= f.text_area :body, placeholder: "本文" %>
<%= f.submit "投稿する" %>
<% end %>